{
  "name": "aloes-client",
  "version": "0.5.0",
  "private": false,
  "main": "src/main.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build": "vue-cli-service build",
    "build:local": "cp deploy/.env_local .env && vue-cli-service build",
    "build:stage": "cp deploy/.env_staging .env && vue-cli-service build --dest build --modern",
    "build:prod": "cp deploy/.env_production .env && vue-cli-service build --dest build --modern",
    "build:docker": "cp deploy/.env_docker .env && docker-compose build",
    "serve": "vue-cli-service serve",
    "start:dev": "cp deploy/.env_development .env && vue-cli-service serve",
    "start:local": "cp deploy/.env_local .env && vue-cli-service serve",
    "start:stage": "cp deploy/.env_staging .env && vue-cli-service serve",
    "start:docker": "cp deploy/.env_docker .env && docker-compose up",
    "serve:local": "cp deploy/.env_local .env; node ./server.js",
    "serve:stage": "cp deploy/.env_staging .env; node ./server.js",
    "deploy:local": "pm2 startOrRestart ecosystem.config.js --env local --update-env",
    "stop:docker": "cp deploy/.env_docker .env && docker-compose down"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "aloes-handlers": "^1.2.13",
    "aloes-logger": "^1.0.4",
    "async-mqtt": "^2.4.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.1",
    "core-js": "^3.3.2",
    "d3-collection": "^1.0.7",
    "d3-drag": "^1.2.3",
    "d3-fetch": "^1.1.2",
    "d3-force": "^2.0.0",
    "d3-hierarchy": "^1.1.8",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^3.2.0",
    "d3-selection": "^1.3.2",
    "d3-transition": "^1.1.3",
    "d3-zoom": "^1.7.3",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.3.4",
    "localtunnel": "^1.9.2",
    "lodash.debounce": "^4.0.8",
    "lodash.has": "^4.5.2",
    "lodash.throttle": "^4.1.1",
    "mqtt-pattern": "^1.2.0",
    "qrcode-generator": "^1.4.1",
    "register-service-worker": "^1.6.2",
    "sensor-snap": "^0.3.1",
    "tiza": "^2.2.1",
    "uid": "0.0.2",
    "vue": "^2.6.10",
    "vue-croppie": "^2.0.1",
    "vue-router": "^3.1.3",
    "vue2-leaflet": "^2.2.1",
    "vuex": "^3.0.1",
    "vuex-cache": "^3.1.0",
    "vuex-persist": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "worker-loader": "^2.0.0"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/html-quotes": "warning",
      "vue/html-self-closing": "warning",
      "vue/no-multi-spaces": "warning",
      "vue/require-prop-types": "warning",
      "vue/no-confusing-v-for-v-if": "warning",
      "vue/no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "/public/",
    "/dist/",
    "/build/",
    "/docs/",
    "/node_modules/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "jsxBracketSameLine": true,
    "trailingComma": "all"
  },
  "prettierIgnore": [
    "build/",
    "/dist/",
    "node_modules/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "https://framagit.org/aloes/aloes-client"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Edouard Maleix <ed@getlarge.eu> (https://getlarge.eu)",
  "contributors": [
    "Edouard Maleix <ed@getlarge.eu> (https://getlarge.eu)"
  ]
}
