{
  "name": "aloes-client",
  "version": "0.6.0",
  "private": false,
  "main": "src/main.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "set-env:dev": "envsub -f ./.env ./public/env.template.js ./public/env.js",
    "set-env:prod": "envsub -f ./.env ./dist/env.template.js ./dist/env.js",
    "prepare:dev:local": "cp deploy/.env_local .env && npm run set-env:dev",
    "prepare:dev:staging": "cp deploy/.env_staging .env && npm run set-env:dev",
    "prepare:prod:local": "cp deploy/.env_local .env && npm run set-env:prod",
    "prepare:prod:staging": "cp deploy/.env_staging .env && npm run set-env:prod",
    "build": "vue-cli-service build --dest dist",
    "build:modern": "vue-cli-service build --dest dist --modern",
    "build:docker": "docker build -t getlarge/aloes-client:staging .",
    "serve": "vue-cli-service serve",
    "start": "light-server -s ./dist -p 8080 --no-reload --historyindex '/index.html' -q",
    "start:docker": "docker run -p 8080:8080 --rm --name aloes-client getlarge/aloes-client:staging",
    "stop:docker": "docker kill aloes-client",
    "publish:docker": "docker push getlarge/aloes-client:staging"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "aloes-handlers": "^1.3.1",
    "aloes-logger": "^1.0.5",
    "async-mqtt": "^2.5.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.2.1",
    "bootstrap-vue": "^2.11.0",
    "core-js": "^3.3.2",
    "d3-collection": "^1.0.7",
    "d3-drag": "^1.2.5",
    "d3-fetch": "^1.1.2",
    "d3-force": "^2.0.1",
    "d3-hierarchy": "^1.1.9",
    "d3-interpolate": "^1.4.0",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-transition": "^1.3.2",
    "d3-zoom": "^1.8.3",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.has": "^4.5.2",
    "lodash.throttle": "^4.1.1",
    "mqtt-pattern": "^1.2.0",
    "oma-json": "^1.1.1",
    "qrcode-generator": "^1.4.4",
    "register-service-worker": "^1.7.1",
    "sensor-snap": "^0.3.1",
    "tiza": "^2.2.1",
    "uid": "0.0.2",
    "vue": "^2.6.11",
    "vue-croppie": "^2.0.1",
    "vue-router": "^3.1.6",
    "vue2-leaflet": "^2.5.2",
    "vuex": "^3.1.3",
    "vuex-cache": "^3.1.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-unit-mocha": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^3.0.0",
    "envsub": "^4.0.7",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "light-server": "^2.6.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "worker-loader": "^2.0.0"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/html-quotes": 1,
      "vue/html-self-closing": 0,
      "vue/no-multi-spaces": 1,
      "vue/require-prop-types": 1,
      "vue/no-confusing-v-for-v-if": 1,
      "vue/no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "/public/",
    "/dist/",
    "/build/",
    "/docs/",
    "/node_modules/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "jsxBracketSameLine": true,
    "trailingComma": "all"
  },
  "prettierIgnore": [
    "build/",
    "/dist/",
    "node_modules/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "https://framagit.org/aloes/aloes-client"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Edouard Maleix <ed@getlarge.eu> (https://getlarge.eu)",
  "contributors": [
    "Edouard Maleix <ed@getlarge.eu> (https://getlarge.eu)"
  ]
}
