{
  "name": "aloes-client",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve",
    "start:dev": "cp deploy/.env_development .env && vue-cli-service serve",
    "start:local": "cp deploy/.env_local .env && vue-cli-service serve",
    "start:stage": "cp deploy/.env_staging .env && vue-cli-service serve",
    "start:docker": "cp deploy/.env_docker .env && docker-compose up",
    "build": "vue-cli-service build --dest build",
    "build:local": "cp deploy/.env_local .env && vue-cli-service build --dest build --modern",
    "build:stage": "cp deploy/.env_staging .env && vue-cli-service build --dest build --modern",
    "build:prod": "cp deploy/.env_production .env && vue-cli-service build --dest build --modern",
    "build:docker": "cp deploy/.env_docker .env && docker-compose build",
    "serve:local": "cp deploy/.env_local .env; node ./server.js ",
    "serve:stage": "cp deploy/.env_staging .env; node ./server.js ",
    "deploy:local": "pm2 startOrRestart ecosystem.config.js --env local --update-env",
    "stop:docker": "docker-compose down"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "aloes-handlers": "^1.2.11",
    "aloes-logger": "^1.0.4",
    "animejs": "^3.0.1",
    "async-mqtt": "^2.0.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.1",
    "d3-collection": "^1.0.7",
    "d3-drag": "^1.2.3",
    "d3-fetch": "^1.1.2",
    "d3-force": "^2.0.0",
    "d3-hierarchy": "^1.1.8",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-transition": "^1.1.3",
    "d3-zoom": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.3.4",
    "localtunnel": "^1.9.2",
    "lodash.debounce": "^4.0.8",
    "lodash.has": "^4.5.2",
    "moment": "^2.22.2",
    "mqtt-pattern": "^1.2.0",
    "qrcode-generator": "^1.4.1",
    "sensor-snap": "^0.2.6",
    "tiza": "^2.2.1",
    "uid": "0.0.2",
    "vue": "^2.6.10",
    "vue-croppie": "^2.0.1",
    "vue-router": "^3.1.3",
    "vue2-editor": "^2.10.2",
    "vue2-leaflet": "^2.2.1",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.1.1",
    "vuex-cache": "^3.1.0",
    "vuex-persist": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-loopback": "^13.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "prettier": "1.16.1",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-editorconfig": "^0.2.1",
    "vue-template-compiler": "^2.5.17"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-quotes": "warning",
      "vue/html-self-closing": "warning",
      "vue/no-multi-spaces": "warning",
      "vue/require-prop-types": "warning",
      "vue/no-confusing-v-for-v-if": "warning",
      "vue/no-console": "off"
    }
  },
  "eslintIgnore": [
    "/public/",
    "/docs/",
    "/node_modules/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "jsxBracketSameLine": true,
    "trailingComma": "all"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://framagit.org/getlarge/aloes-client"
  }
}
